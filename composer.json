{
	"name": "hypejunction/hypewall",
	"type": "elgg-plugin",
	"description": "Rich status updates",
	"homepage": "http://hypejunction.com",
	"keywords": [
		"elgg", "plugin", "wall", "posts", "embed", "link", "images"
	],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "Ismayil Khayredinov",
			"email": "ismayil.khayredinov@gmail.com"
		}
	],
	"extra": {
		"installer-name": "hypeWall"
	},
	"config": {
		"vendor-dir": "vendors",
		"preferred-install": "dist"
	},
	"suggest": {
		"hypejunction/hypeembed" : "@stable",
		"hypejunction/elgg_dropzone" : "@stable"
	},
	"require": {
		"php": ">=5.3.3",
		"composer/installers": "1.0.*"
	},
	"require-dev": {
		"hypejunction/cmp-filestore": "1.0.*",
		"smottt/wideimage": "dev-master#658e6b8699255724589e8fb86959306ec64af318"
	},
	"autoload": {
		"psr-4": {
			"hypeJunction\\": "classes/hypeJunction/",
			"UFCOE\\": "classes/UFCOE/"
		}
	}
}

